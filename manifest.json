{
  "name": "wyxt",
  "version": "1.0.0",
  "description": "wyxt Extension!",
  "manifest_version": 2 ,
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "images/16.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://wywk.yunxuetang.cn/*",
        "https://wywk.yunxuetang.cn/*"
      ],
      "js": [
        "scripts/content.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ]
}
